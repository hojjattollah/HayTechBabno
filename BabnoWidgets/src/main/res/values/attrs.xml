<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DoubleText">
        <attr name="leftText" format="string" />
        <attr name="rightText" format="string" />
        <attr name="android:ems" />

    </declare-styleable>

    <declare-styleable name="HamburgerButtonMenu">
        <attr name="cv_borderColor" format="color" />
        <attr name="cv_borderSize" format="dimension" />
        <attr name="cv_borderRadius" format="dimension" />
        <attr name="cv_iconSource" format="reference" />
        <attr name="cv_iconScaleType">
            <!-- Scale using the image matrix when drawing. See
                 {@link android.widget.ImageView#setImageMatrix(Matrix)}. -->
            <enum name="matrix" value="0" />
            <!-- Scale the image using {@link android.graphics.Matrix.ScaleToFit#FILL}. -->
            <enum name="fitXY" value="1" />
            <!-- Scale the image using {@link android.graphics.Matrix.ScaleToFit#START}. -->
            <enum name="fitStart" value="2" />
            <!-- Scale the image using {@link android.graphics.Matrix.ScaleToFit#CENTER}. -->
            <enum name="fitCenter" value="3" />
            <!-- Scale the image using {@link android.graphics.Matrix.ScaleToFit#END}. -->
            <enum name="fitEnd" value="4" />
            <!-- Center the image in the view, but perform no scaling. -->
            <enum name="center" value="5" />
            <!-- Scale the image uniformly (maintain the image's aspect ratio) so both dimensions
                 (width and height) of the image will be equal to or larger than the corresponding
                 dimension of the view (minus padding). The image is then centered in the view. -->
            <enum name="centerCrop" value="6" />
            <!-- Scale the image uniformly (maintain the image's aspect ratio) so that both
                 dimensions (width and height) of the image will be equal to or less than the
                 corresponding dimension of the view (minus padding). The image is then centered in
                 the view. -->
            <enum name="centerInside" value="7" />
        </attr>
        <attr name="android:ems" />

    </declare-styleable>

    <declare-styleable name="FilterBox">
        <attr name="cv_sb_borderEmptyStateColor" format="color" />
        <attr name="cv_sb_borderEmptyStateSize" format="dimension" />
        <attr name="cv_sb_borderEmptyStateRadius" format="dimension" />
        <attr name="cv_sb_borderEmptyStateDashWith" format="dimension" />
        <attr name="cv_sb_borderEmptyStateDashGap" format="dimension" />

        <attr name="cv_sb_borderTextStateColor" format="color" />
        <attr name="cv_sb_borderTextStateSize" format="dimension" />
        <attr name="cv_sb_borderTextStateRadius" format="dimension" />
        <attr name="cv_sb_borderTextStateDashWith" format="dimension" />
        <attr name="cv_sb_borderTextStateDashGap" format="dimension" />

        <attr name="cv_sb_searchIconVisibility">
            <!-- Visible on screen; the default value. -->
            <enum name="visible" value="0" />
            <!-- Not displayed, but taken into account during layout (space is left for it). -->
            <enum name="invisible" value="1" />
            <!-- Completely hidden, as if the view had not been added. -->
            <enum name="gone" value="8" />
        </attr>
        <attr name="cv_sb_searchIconSource" format="reference" />
        <attr name="cv_sb_searchIconScaleType">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>

        <attr name="cv_sb_clearIconVisibility">
            <!-- Visible on screen; the default value. -->
            <enum name="visible" value="0" />
            <!-- Not displayed, but taken into account during layout (space is left for it). -->
            <enum name="invisible" value="1" />
            <!-- Completely hidden, as if the view had not been added. -->
            <enum name="gone" value="8" />
        </attr>
        <attr name="cv_sb_clearIconSource" format="reference" />
        <attr name="cv_sb_clearIconScaleType">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
        <attr name="android:ems" />

    </declare-styleable>

    <declare-styleable name="SearchBox">
        <attr name="cv_sv_containerEmptyStateBackground" format="reference" />
        <attr name="cv_sv_containerTextStateBackground" format="reference" />
        <attr name="cv_sv_containerPaddingLeft" format="dimension" />
        <attr name="cv_sv_editTextBackground" format="reference" />
        <attr name="cv_sv_editTextHint" format="string" />
        <attr name="cv_sv_editTextContent" format="string" />
        <attr name="cv_sv_editTextPaddingRight" format="dimension" />
        <attr name="cv_sv_editTextPaddingLeft" format="dimension" />
        <attr name="cv_sv_searchButtonBackground" format="reference" />
        <attr name="cv_sv_searchButtonText" format="string" />
        <attr name="cv_sv_searchIconSource" format="reference" />
        <attr name="cv_sv_searchButtonVisibility">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="8" />
        </attr>
        <attr name="android:ems" />

    </declare-styleable>


    <declare-styleable name="ToastStyleable">
        <attr name="cv_st_TextBold" format="boolean" />
        <attr name="cv_st_TextColor" format="color" />
        <attr name="cv_st_Font" format="reference" />
        <attr name="cv_st_TextSize" format="dimension" />
        <attr name="cv_st_ColorBackground" format="color" />
        <attr name="cv_st_SolidBackground" format="boolean" />
        <attr name="cv_st_Radius" format="dimension" />
        <attr name="cv_st_StrokeWidth" format="dimension" />
        <attr name="cv_st_StrokeColor" format="color" />
        <attr name="cv_st_IconStart" format="reference" />
        <attr name="cv_st_IconEnd" format="reference" />
        <attr name="cv_st_Length" format="integer">
            <enum name="LONG" value="1" />
            <enum name="SHORT" value="0" />
        </attr>
        <attr name="cv_st_Gravity" format="integer">
            <enum name="center" value="1" />
            <enum name="top" value="2" />
        </attr>

    </declare-styleable>


    <declare-styleable name="ErrorPage">
        <attr name="cv_ep_errorIconSource" format="reference" />
        <attr name="cv_ep_errorIconWidth" format="dimension" />
        <attr name="cv_ep_errorIconHeight" format="dimension" />
        <attr name="cv_ep_errorIconVisibility">
            <enum name="visible" value="0" />
            <enum name="gone" value="8" />
        </attr>
        <attr name="cv_ep_errorImageSource" format="reference" />
        <attr name="cv_ep_errorImageWidth" format="dimension" />
        <attr name="cv_ep_errorImageHeight" format="dimension" />
        <attr name="cv_ep_errorImageVisibility">
            <enum name="visible" value="0" />
            <enum name="gone" value="8" />
        </attr>

        <attr name="cv_ep_errorTitleBackground" format="reference" />
        <attr name="cv_ep_errorTitleText" format="string" />
        <attr name="cv_ep_errorTitleTextSize" format="dimension" />
        <attr name="cv_ep_errorTitleTextColor" format="color" />
        <attr name="cv_ep_errorTitleTextPaddingTop" format="dimension" />

        <attr name="cv_ep_trackingCodeLableBackground" format="reference" />
        <attr name="cv_ep_trackingCodeLableText" format="string" />
        <attr name="cv_ep_trackingCodeLableTextSize" format="dimension" />
        <attr name="cv_ep_trackingCodeLableTextColor" format="color" />
        <attr name="cv_ep_trackingCodeLableTextPaddingTop" format="dimension" />

        <attr name="cv_ep_trackingCodeValueBackground" format="reference" />
        <attr name="cv_ep_trackingCodeValueText" format="string" />
        <attr name="cv_ep_trackingCodeValueTextSize" format="dimension" />
        <attr name="cv_ep_trackingCodeValueTextColor" format="color" />
        <attr name="cv_ep_trackingCodeValueTextPaddingTop" format="dimension" />

        <attr name="cv_ep_errorDescriptionBackground" format="reference" />
        <attr name="cv_ep_errorDescriptionText" format="string" />
        <attr name="cv_ep_errorDescriptionTextSize" format="dimension" />
        <attr name="cv_ep_errorDescriptionTextColor" format="color" />
        <attr name="cv_ep_errorDescriptionTextPaddingTop" format="dimension" />

        <attr name="cv_ep_errorTitleDescriptionBackground" format="reference" />
        <attr name="cv_ep_errorTitleDescriptionText" format="string" />
        <attr name="cv_ep_errorTitleDescriptionTextSize" format="dimension" />
        <attr name="cv_ep_errorTitleDescriptionTextColor" format="color" />
        <attr name="cv_ep_errorTitleDescriptionTextPaddingTop" format="dimension" />

        <attr name="cv_ep_firstButtonBackground" format="reference" />
        <attr name="cv_ep_firstButtonText" format="string" />
        <attr name="cv_ep_firstButtonTextSize" format="dimension" />
        <attr name="cv_ep_firstButtonTextColor" format="color" />
        <attr name="cv_ep_firstButtonVisibility">
            <enum name="visible" value="0" />
            <enum name="gone" value="8" />
        </attr>
        <attr name="cv_ep_secondButtonBackground" format="reference" />
        <attr name="cv_ep_secondButtonText" format="string" />
        <attr name="cv_ep_secondButtonTextSize" format="dimension" />
        <attr name="cv_ep_secondButtonTextColor" format="color" />
        <attr name="cv_ep_secondButtonVisibility">
            <enum name="visible" value="0" />
            <enum name="gone" value="8" />
        </attr>
        <attr name="cv_ep_errorType">
            <enum name="no_server_connection" value="0" />
            <enum name="phone_not_support" value="1" />
            <enum name="no_internet_connection" value="2" />
            <enum name="important_update" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MultiButtonView">
        <attr name="cv_mbv_itemCount" format="integer" />
        <attr name="cv_mbv_itemSpace" format="integer" />
        <attr name="cv_mbv_itemColumn" format="integer" />
        <attr name="cv_mbv_itemDefaultSelect" format="integer" />
    </declare-styleable>
</resources>
